{
  "name": "gulp-bro",
  "version": "0.3.1",
  "description": "gulp + browserify + incremental build, done right.",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/gulp-bro",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "eslint {,test/}*.js && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | codecov",
    "check-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95",
    "contributors": "contributor-faces --exclude '*-bot'"
  },
  "precommit": [
    "check-coverage",
    "test",
    "contributors"
  ],
  "keywords": [
    "browserify",
    "gulp",
    "recipe",
    "simple",
    "stream",
    "task",
    "watchify"
  ],
  "dependencies": {
    "browserify": "^13.1.0",
    "browserify-incremental": "^3.1.1",
    "concat-stream": "^1.5.2",
    "gulp-util": "^3.0.7",
    "into-stream": "^3.0.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.2.0",
    "catch-stdout": "0.0.1",
    "chokidar": "^1.4.3",
    "codecov.io": "^0.1.6",
    "contributor-faces": "^0.4.0",
    "eslint": "^3.7.0",
    "eslint-config-ngryman": "^1.6.0",
    "nyc": "^8.3.0",
    "pre-commit": "^1.1.2",
    "stream-assert": "^2.0.3",
    "vinyl-fs": "^2.3.1"
  }
}
